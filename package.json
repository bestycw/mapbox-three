{
  "name": "mapbox-three",
  "version": "0.1.0",
  "description": "A Three.js plugin for Mapbox GL JS",
  "main": "dist/mapbox-three.js",
  "module": "dist/mapbox-three.mjs",
  "types": "dist/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "mapbox-gl": ">=3.1.2",
    "three": ">=0.162.0"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^3.1.2",
    "@types/node": "^20.11.24",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "mapbox-gl": ">=3.1.2",
    "three": ">=0.162.0"
  }
}
